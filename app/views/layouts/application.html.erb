<!DOCTYPE html>
<html>
  <head>
    <title>Moviestream</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag asset_path('fav.png') %>
    </head>

  <body>
      <div class="logo">
         
      <%= image_tag("mslogo.png",  :alt =>"MovieStream logo" ) %>
      </div>    <%= link_to "Please find our packages", packages_path %> | 
      <%= link_to "Find a Customer", customers_path %> |
      <%=link_to 'Log in', login_path %> |
 <p><%=link_to 'Log out', logout_path %>
</p>
    <%= yield %>
  </body>
</html>
